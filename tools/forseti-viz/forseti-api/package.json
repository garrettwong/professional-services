{
  "name": "docker_web_app",
  "version": "1.0.0",
  "description": "Node.js on Docker",
  "author": "Garrett Wong",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node server --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "serve": "nodemon -w src --exec \"babel-node server --presets es2015,stage-0\"",
    "start": "npm run serve",
    "test": "eslint src",
    "server_bigtable_tests": "nodemon -w src --exec \"babel-node server_bigtable_tests --presets es2015,stage-0\""
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "dependencies": {
    "@google-cloud/bigtable": "^0.15.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cors": "^2.8.4",
    "express": "^4.16.1",
    "morgan": "^1.9.0",
    "mysql": "^2.16.0",
    "pug": "^2.0.3",
    "resource-router-middleware": "^0.7.0"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/npm/npm.git"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^5.3.0",
    "nodemon": "^1.18.3"
  }
}
